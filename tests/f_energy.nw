start f_energy
title "F- gas energy"
memory total 1000 mb
scratch_dir ./scratch
permanent_dir ./perm

echo

charge -1
geometry units angstrom
 F 0 0 0
end

basis
    * library def2-svpd
end

dft
  xc b3lyp
  mult 1
  grid xfine
  iterations 200
end
task dft energy

